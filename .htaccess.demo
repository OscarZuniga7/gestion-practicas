AuthType Basic
AuthName "Acceso temporal"
AuthUserFile "C:/xampp/htpasswd/.htpasswd"
Require valid-user

<IfModule mod_access_compat.c>
  <LimitExcept GET HEAD OPTIONS>
    Deny from all
  </LimitExcept>
</IfModule>

<IfModule !mod_access_compat.c>
  <LimitExcept GET HEAD OPTIONS>
    Require all denied
  </LimitExcept>
</IfModule>

<IfModule mod_headers.c>
  Header always set X-Robots-Tag "noindex, nofollow"
</IfModule>
Options -Indexes
